[ValkyrianShoes]
	Name=Valkyrian Shoes
	HP=if ( ($readini($char($1), stats, str) < ($readini($char($1), stats, int) ) { $round($calc(5 * ($get.level($1)),0) } else { 0 }
	TP=if ( ($readini($char($1), stats, str) > ($readini($char($1), stats, int) ) { $round($calc(2 * ($get.level($1)),0) } else { 0 }
